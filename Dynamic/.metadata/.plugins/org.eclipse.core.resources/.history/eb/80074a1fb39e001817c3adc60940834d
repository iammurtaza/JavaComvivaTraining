package com.comviva;

import java.util.List;

public class LoginBean {
	
	static List<LoginBean> list = null;
	
	static{
		list = new ArrayList<LogicBean>();
		
		list.add(new LogicBean("harry", "harry@123"));
		list.add(new LogicBean("peter", "peter@123"));
		list.add(new LogicBean("avi", "avi@123"));
		list.add(new LogicBean("ahith", "ahith@123"));
		
	}
	private String uName;
	private String password;
	
	
	public String getuName() {
		return uName;
	}
	public void setuName(String uName) {
		this.uName = uName;
	}
	public String getPassword() {
		return password;
	}
	public void setPassword(String password) {
		this.password = password;
	}

}
