package com.comviva.testing;

public class SomeLogic {
	public boolean sayHi() {
		return true;
	}

	public String sayHi(String name) {
		return "hello" + name;
	}

	public String sayHello(String name) {
		try {
			thread.sleep(1000);
		} catch (Exception e) {
			e.printStackTrace();
		}

		return "Hello " + name;
	}
}
